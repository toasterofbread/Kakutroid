[application]

config/name="Kakutroid"
run/main_scene="res://src/scenes/rooms/test_area/test/Room.tscn"
config/icon="res://icon.png"

[autoload]

Utils="*res://addons/Godot-Utilities/Utils.gd"
InputManager="*res://src/scripts/singletons/InputManager.gd"
Enums="*res://src/scripts/singletons/Enums.gd"
Game="*res://src/scripts/singletons/Game.gd"
NotificationManager="*res://src/scripts/singletons/notification/NotificationDisplay.tscn"
Overlay="*res://src/scripts/singletons/debug_overlay/overlay.gd"
HyperLog="*res://addons/hyperlog/hyperlog.tscn"

[debug]

gdscript/completion/autocomplete_setters_and_getters=true
gdscript/warnings/shadowed_variable=false
gdscript/warnings/incompatible_ternary=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false
gdscript/warnings/integer_division=false
shapes/collision/max_contacts_displayed=0

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor]

search_in_file_extensions=PoolStringArray( "gd", "gdh", "shader", "cs" )

[editor_plugins]

enabled=PoolStringArray( "res://addons/Godot-Utilities/plugin.cfg", "res://addons/Mono-Build-Toggler/plugin.cfg", "res://addons/SingletonScriptsShortcut/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/UndoRedo-History-Viewer/plugin.cfg", "res://addons/godot-notes/plugin.cfg", "res://addons/godot-plugin-refresher/plugin.cfg", "res://addons/godot_sfxr/plugin.cfg", "res://addons/hyperlog/plugin.cfg", "res://addons/launchy/plugin.cfg", "res://addons/project-statistics/plugin.cfg", "res://addons/todot/plugin.cfg", "res://addons/wakatime/plugin.cfg" )

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

